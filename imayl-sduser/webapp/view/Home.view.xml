<mvc:View controllerName="com.db.sdu.imaylsduser.controller.Home"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:uxap="sap.uxap"
    xmlns:cards="sap.f.cards"
    xmlns:unified="sap.ui.unified"
    xmlns:table="sap.ui.table"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:vk="sap.ui.vk"
    xmlns:f="sap.f"
    xmlns:tnt="sap.tnt"   
    xmlns:rowmodes="sap.ui.table.rowmodes">
                    <Page id="MyPackagespage">
                        <customHeader>
                            <Toolbar>
                                <Title
                                    text="My Packages
"
                                    level="H1"
                                />
                                <ToolbarSpacer />

                                <!-- <Button
                                    icon="sap-icon://refresh"
                                    type="Critical"
                                /> -->
                                <Button
                                    icon="sap-icon://filter"
                                    text="Filter"
                                    type="Emphasized"
                                    press="onFilterselection"
                                />
                                <Button
                                    icon="sap-icon://add"
                                    type="Emphasized"
                                    press="onAddReceiveAPackage"
                                />
                                <!-- <Button
                                    icon="sap-icon://action-settings"
                                    type="Accept"
                                /> -->
                            </Toolbar>
                        </customHeader>
                        <Panel>
                            <Bar>
                                <contentLeft>
                                    <Title
                                        text="Total Packages: 0 in Last {
                                parts: [
                                         {path: 'requestpackageModel>/popoverfromdate'},
                                          {path: 'requestpackageModel>/popovertoDate'}
                                        ],
                                        formatter: '.calculateDays'
                                } Days"
                                    />
                                </contentLeft>
                                <contentRight>
                                    <Title
                                        text="{
                                parts: [
                                         {path: 'requestpackageModel>/popoverfromdate'},
                                          {path: 'requestpackageModel>/popovertoDate'}
                                        ],
                                        formatter: '.formatDateRange'
                                }"
                                    />
                                    <!-- <Title text="{requestpackageModel>/popoverfromdate} - {requestpackageModel>/popovertoDate}" /> -->
                                </contentRight>
                            </Bar>
                            <HBox
                                fitContainer="true"
                                width="100%"
                                justifyContent="SpaceAround"
                            >
                                <GenericTile
                                    class="allpackagesReceived"
                                    header="Received"
                                    frameType="TwoByHalf"
                                    press="press"
                                    width="13rem"
                                >
                                    <TileContent>
                                        <NumericContent
                                            value="12"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                                <GenericTile
                                    class="allpackagesTender"
                                    header="Tender"
                                    frameType="TwoByHalf"
                                    press="press"
                                    width="12rem"
                                >
                                    <TileContent
                                        unit="Unit"
                                        footer="Footer Text"
                                    >
                                        <NumericContent
                                            value="12"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                                <GenericTile
                                    class="allpackagesResearch"
                                    header="Research"
                                    frameType="TwoByHalf"
                                    press="press"
                                    width="12rem"
                                >
                                    <TileContent
                                        unit="Unit"
                                        footer="Footer Text"
                                    >
                                        <NumericContent
                                            value="12"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                                <GenericTile
                                    class="allpackagesDelivered"
                                    header="Delivered"
                                    frameType="TwoByHalf"
                                    press="press"
                                    width="12rem"
                                >
                                    <TileContent
                                        unit="Unit"
                                        footer="Footer Text"
                                    >
                                        <NumericContent
                                            value="12"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                                <GenericTile
                                    class="allpackagesForward"
                                    header="Forward"
                                    frameType="TwoByHalf"
                                    press="press"
                                    width="12rem"
                                >
                                    <TileContent
                                        unit="Unit"
                                        footer="Footer Text"
                                    >
                                        <NumericContent
                                            value="12"
                                            withMargin="false"
                                        />
                                    </TileContent>
                                </GenericTile>
                            </HBox>
                        </Panel>
                        <ToolbarSpacer />
                        <table:Table selectionMode="None">
                            <table:extension>
                                <OverflowToolbar>
                                    <ToolbarSpacer />
                                    <Button
                                        icon="sap-icon://action-settings"
                                        press="onOpenColumnftlter"
                                    />
                                </OverflowToolbar>
                            </table:extension>
                            <table:columns>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Color Code" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Received Date" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Updated Date" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Ship From" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="From Address" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="To Address" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Carrier" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Ext #" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Int #" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Status" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Ref #" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Ref Date" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Delivery Location" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                                <table:Column
                                    width="9rem"
                                    filterProperty="color"
                                >
                                    <Label text="Notes" />
                                    <table:template>
                                        <Text
                                            text="color"
                                            wrapping="false"
                                        />
                                    </table:template>
                                </table:Column>
                            </table:columns>
                        </table:Table>
                    </Page>

</mvc:View>